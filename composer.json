{
  "name": "allejo/php-soda",
  "description": "A PHP library for working with the Socrata API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "autoload": {
    "psr-4": {
      "allejo\\Socrata\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/utilities"
    ]
  },
  "authors": [
    {
      "name": "Vladimir Jimenez",
      "homepage": "https://allejo.io",
      "role": "Project Maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/allejo/PhpSoda/issues",
    "irc": "irc://irc.freenode.net/socrata-soda",
    "wiki": "https://github.com/allejo/PhpSoda/wiki",
    "source": "https://github.com/allejo/PhpSoda"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
